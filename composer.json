{
  "name": "ibrand/laravel-database-logger",
  "type": "library",
  "description": "ibrand laravel database logger",
  "keywords": [
    "ibrand",
    "laravel",
    "database logger"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "shjchen",
      "email": "ibrand.shjchen@foxmail.com"
    }
  ],
  "require": {
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.5",
    "orchestra/database": "~3.5"
  },
  "autoload": {
    "psr-4": {
      "iBrand\\DatabaseLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "iBrand\\DatabaseLogger\\Test\\": "tests/"
    }
  },

  "extra": {
    "laravel": {
      "providers": [
        "iBrand\\DatabaseLogger\\ServiceProvider"
      ]
    }
  }
}